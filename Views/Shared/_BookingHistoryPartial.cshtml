@model IEnumerable<WebAppHotelManagement.ViewModel.RoomBookingViewModel>

<table style="width: 100%" border="1" bordercolor="grey">
    <thead>
        <tr bgcolor="peru">
            <th>
                Customer Name
            </th>
            <th>
                Customer Address
            </th>
            <th>
                Customer Phone
            </th>
            <th>
                Number of Members
            </th>
            <th>
                Room Number
            </th>
        </tr>
    </thead>

    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.CustomerName
                </td>
                <td>
                    @item.CustomerAddress
                </td>
                <td>
                    @item.CustomerPhone
                </td>

                <td>
                    @item.NumberOfMembers
                </td>
                <td>
                    @item.RoomNumber
                </td>
            </tr>
        }
    </tbody>
</table>



